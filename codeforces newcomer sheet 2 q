#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t)
    {
        int n,rem=0;
        cin>>n;
        if(n==0)
            cout<<0;
        while(n!=0)
        {
            rem=n%10;
            n=n/10;
            cout<<rem<<" ";

        }
        cout<<"\n";



    t--;
    }
}
